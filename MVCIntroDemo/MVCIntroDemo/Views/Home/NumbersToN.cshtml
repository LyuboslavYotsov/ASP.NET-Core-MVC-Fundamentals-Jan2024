@{
    ViewBag.Title = "Nums 1 ..." + ViewBag.Count;
}

<h2>@ViewBag.Title</h2>

<ul>
    @for (int i = 1; i <= ViewBag.Count; i++)
    {
        <li>@i</li>
    }
</ul>

<form method="post" class="align-content-center">
    <input name="count" type="number"/>
    <input  type="submit" value="Submit"/>
</form>

